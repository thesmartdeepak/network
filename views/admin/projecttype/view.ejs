<% layout('../layout/layout') -%>
<!-- Main content -->
<section class="content" ng-controller="ctrl">
    <div class="box box-primary">
        <table class="table">
            <thead>
                <th>Name</th>
               <th>Department</th>
                 <!-- <th>Code</th> -->
                <th>Action</th>
            </thead>
            <tbody>
                <tr ng-repeat="projecttype in projecttypes">
                    <td>{{ projecttype.name }}</td>
                    <td>{{ projecttype.departmentId }}</td>
                    <!-- <td>{{ projecttype.description }}</td>
                    <td>{{ projecttype.code }}</td> -->
                    <td>
                        <a class="btn btn-info" href="/edit-projecttype?projecttypeId={{ projecttype._id }}">Edit</a>
                        <a class="btn btn-danger" ng-click="deleteprojecttype(projecttype._id)">Delete</a>
                    </td>
                </tr>
            </tbody>
        </table>
        <ul id="pagination" class="pagination-sm"></ul>
    </div>
</section>

<script src="/admin/js/jquery.twbsPagination.min.js"></script>
<script src="/admin/js/projecttype/view.js"></script>